{
  "name": "mono-angular",
  "version": "0.0.0",
  "scripts": {
    "ci:env": "ts-node ./build/set-env-ci.ts",
    "init:env-local": "ts-node ./build/create-local-env.ts",
    "init:git-hooks": "simple-git-hooks",
    "init": "npm run init:git-hooks && npm run init:env-local",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "assets:build": "ts-node ./projects/ui/src/build/runBuild.ts",
    "sb:start": "ts-node build/run-storybook.ts start",
    "sb:build": "ts-node build/run-storybook.ts build",
    "sb:update": "ts-node build/update-storybook.ts",
    "angular:update": "ts-node build/update-angular.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^21.1.3",
    "@angular/compiler": "^21.1.3",
    "@angular/core": "^21.1.3",
    "@angular/forms": "^21.1.3",
    "@angular/platform-browser": "^21.1.3",
    "@angular/platform-browser-dynamic": "^21.1.3",
    "@angular/platform-server": "^21.1.3",
    "@angular/router": "^21.1.3",
    "@angular/ssr": "^21.1.4",
    "@date-fns/tz": "^1.4.1",
    "@floating-ui/dom": "^1.7.5",
    "@justinribeiro/lite-youtube": "^1.9.0",
    "date-fns": "^4.1.0",
    "express": "^5.2.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zod": "^4.1.13",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.4",
    "@angular/cli": "^21.1.4",
    "@angular/compiler-cli": "^21.1.3",
    "@chromatic-com/storybook": "^5.0.1",
    "@compodoc/compodoc": "^1.2.1",
    "@storybook/addon-a11y": "10.2.8",
    "@storybook/addon-designs": "^11.1.2",
    "@storybook/addon-docs": "10.2.8",
    "@storybook/addon-themes": "10.2.8",
    "@storybook/angular": "10.2.8",
    "@types/express": "^5.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/google.accounts": "^0.0.18",
    "@types/jasmine": "^5.1.15",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "@types/semver": "^7.7.1",
    "angular-eslint": "^21.2.0",
    "eslint": "^9.39.2",
    "eslint-plugin-storybook": "10.2.8",
    "fs-extra": "^11.3.3",
    "jasmine-core": "^5.13.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^5.1.0",
    "karma-jasmine-html-reporter": "^2.2.0",
    "nano-staged": "^0.9.0",
    "ng-packagr": "^21.1.0",
    "prettier": "^3.8.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "semver": "^7.7.3",
    "simple-git-hooks": "^2.13.1",
    "storybook": "10.2.8",
    "svgo": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3"
  },
  "simple-git-hooks": {
    "pre-commit": "./pre-commit.sh"
  },
  "nano-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ],
    "*.{js,ts,json,css,md,mdx,html}": [
      "prettier --write"
    ]
  }
}
