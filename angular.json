{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "cli": {
    "schematicCollections": ["angular-eslint"]
  },
  "projects": {
    "ui": {
      "projectType": "library",
      "root": "projects/ui",
      "sourceRoot": "projects/ui/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ui/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ui/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/ui/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": "projects/ui/.storybook",
            "browserTarget": "ui:build",
            "styles": [
              "projects/ui/src/css/reset.css",
              "projects/ui/src/css/theme-default.css"
            ],
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "projects/ui",
              "--disablePrivate",
              "--disableProtected",
              "--disableInternal",
              "--disableLifeCycleHooks"
            ],
            "port": 6006
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": "projects/ui/.storybook",
            "browserTarget": "ui:build",
            "styles": [
              "projects/ui/src/css/reset.css",
              "projects/ui/src/css/theme-default.css"
            ],
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "projects/ui",
              "--disablePrivate",
              "--disableProtected",
              "--disableInternal",
              "--disableLifeCycleHooks"
            ],
            "outputDir": "dist/storybook"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/ui/**/*.ts",
              "projects/ui/**/*.html"
            ],
            "eslintConfig": "projects/ui/eslint.config.js"
          }
        }
      }
    },
    "utils": {
      "projectType": "library",
      "root": "projects/utils",
      "sourceRoot": "projects/utils/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular/build:ng-packagr",
          "options": {
            "project": "projects/utils/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/utils/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/utils/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "tsConfig": "projects/utils/tsconfig.spec.json",
            "polyfills": ["zone.js", "zone.js/testing"]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/utils/**/*.ts",
              "projects/utils/**/*.html"
            ],
            "eslintConfig": "projects/utils/eslint.config.js"
          }
        }
      }
    },
    "countdown": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/countdown",
      "sourceRoot": "projects/countdown/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/countdown",
            "index": "projects/countdown/src/index.html",
            "browser": "projects/countdown/src/main.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "projects/countdown/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/countdown/public"
              }
            ],
            "styles": [
              "projects/ui/src/css/reset.css",
              "projects/ui/src/css/theme-default.css",
              "projects/countdown/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            },
            "local": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "fileReplacements": [
                {
                  "replace": "projects/countdown/src/environments/environment.ts",
                  "with": "projects/countdown/src/environments/environment.local.ts"
                }
              ]
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "options": {
            "port": 4400
          },
          "configurations": {
            "production": {
              "buildTarget": "countdown:build:production"
            },
            "development": {
              "buildTarget": "countdown:build:development"
            },
            "local": {
              "buildTarget": "countdown:build:local"
            }
          },
          "defaultConfiguration": "local"
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/countdown/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/countdown/public"
              }
            ],
            "styles": [
              "projects/ui/src/css/reset.css",
              "projects/ui/src/css/theme-default.css",
              "projects/countdown/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/countdown/**/*.ts",
              "projects/countdown/**/*.html"
            ],
            "eslintConfig": "projects/countdown/eslint.config.js"
          }
        },
        "storybook": {
          "builder": "@storybook/angular:start-storybook",
          "options": {
            "configDir": "projects/countdown/.storybook",
            "browserTarget": "countdown:build",
            "styles": [
              "projects/ui/src/css/reset.css",
              "projects/ui/src/css/theme-default.css",
              "projects/countdown/src/styles.css"
            ],
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "projects/countdown",
              "--disablePrivate",
              "--disableProtected",
              "--disableInternal",
              "--disableLifeCycleHooks"
            ],
            "port": 6016
          }
        },
        "build-storybook": {
          "builder": "@storybook/angular:build-storybook",
          "options": {
            "configDir": "projects/countdown/.storybook",
            "browserTarget": "countdown:build",
            "styles": [
              "projects/ui/src/css/reset.css",
              "projects/ui/src/css/theme-default.css",
              "projects/countdown/src/styles.css"
            ],
            "compodoc": true,
            "compodocArgs": [
              "-e",
              "json",
              "-d",
              "projects/countdown",
              "--disablePrivate",
              "--disableProtected",
              "--disableInternal",
              "--disableLifeCycleHooks"
            ],
            "outputDir": "dist/storybook/countdown"
          }
        }
      }
    },
    "vampire-survivors": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/vampire-survivors",
      "sourceRoot": "projects/vampire-survivors/src",
